{
  "name": "bonefx",
  "version": "0.1.0",
  "description": "A powerful 2D pseudo-3D animation tool with parallax mapping and bone animation support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --out docs/api src/index.ts",
    "build:unity": "tsc -p tsconfig.unity.json",
    "build:unreal": "tsc -p tsconfig.unreal.json",
    "download-model": "node scripts/download-model.js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.16",
    "gl-matrix": "^3.4.3",
    "i18next": "^25.2.0",
    "konva": "^8.4.2",
    "onnxruntime-web": "^1.14.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.1",
    "react-konva": "^18.2.10",
    "three": "^0.150.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.0.11",
    "@types/three": "^0.150.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.23",
    "typescript": "^5.0.3",
    "vite": "^4.2.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "2d",
    "3d",
    "parallax",
    "normal-map",
    "animation",
    "webgl",
    "unity",
    "unreal",
    "editor"
  ],
  "author": "",
  "license": "MIT"
}
